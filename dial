modem
debug
#logfile /var/ppplog 
nodetach 
/dev/ttyUSB2
115200
receive-all
#defaultroute
noipdefault
-chap
usepeerdns ##使用pppoe的DNS,否则改/etc/resolv.conf文件
#mtu 1500
-crtscts
ipcp-accept-local
ipcp-accept-remote
lock
noauth
novj
nobsdcomp
novjccomp
nopcomp
noaccomp
asyncmap 0
user "card"  ####设置用户名
password "card" ####设置密码

#lcp-echo-failure 0
#lcp-echo-interval 0

# Chat scripts
connect "/usr/sbin/chat -v -s -f /etc/ppp/chat"

